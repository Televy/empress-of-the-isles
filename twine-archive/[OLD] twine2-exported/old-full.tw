      var totComStarv=0;
      var totNobStarv=0;
      var totCriStarv=0;
      
      var collapse=0;
      
      var plagueMF=0;
      
      var understaffing=0;
      
      for (var i=0; i<watchPost.length; i++){
        //watch outpost
        if(watchPost[i]==1){
	        watch+=5;
    	    crime-=5;
          totOutpost+=5;
        }
        
        //infection
        if (districtInf[i]/districtPop[i]>0.05){
          if (districtClass[i]=="Commoners"){
            com-=5;
