      for (var i = 0; i < lockdown.length; i++) {
        if (lockdown[i]==2){
            elixir[i]+=Math.floor(modif[i]*prod[i]/2);
        }
        else{
          elixir[i]+=Math.floor(modif[i]*prod[i]);
