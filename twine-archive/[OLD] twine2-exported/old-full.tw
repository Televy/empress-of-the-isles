      }

      var actualEl=State.variables.actualElD;
      actualEl=[...elDist];

      for (var i = 0; i < elixir.length; i++) {
        if (elixir[i]>=elCost[i]){ //if there's enough elixir
          elixir[i]-=elCost[i];//remove the spending
        }
        else{
          actualEl[i]=0;
          alert+='<br>'+name[i]+'</br>';
